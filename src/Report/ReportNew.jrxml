	<queryString>
		<![CDATA[select tsiswa.id as "Student Id", tsiswa.name, tsiswa.class, tclass.classname, tsiswa.StartingDate, tteacher.name as "Teacher Name", tclass.time
from tsiswa
inner join tclass on tclass.id=tsiswa.class
inner join tteacher on tteacher.id=tclass.teacherid
where month(tsiswa.StartingDate)=month(Current_date()) and
year(tsiswa.StartingDate)=year(Current_date()) and
not tsiswa.class='dis';]]>
	</queryString>
