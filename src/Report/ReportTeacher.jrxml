	<queryString>
		<![CDATA[SELECT
     tteacher.`Id` AS Id,
     tteacher.`Name` AS Name,
     tteacher.`Gender` AS Gender,
     tteacher.`Address` AS Address,
     tteacher.`Phone` AS Phone,
     tteacher.`Birthday` AS Birthday,
     tteacher.`Birthplace` AS Birthplace,
     tteacher.`Religion` AS Religion,
     tteacher.`StartingDate` AS StartingDate,
     tteacher.`QuitDate` AS QuitDate,
     tteacher.`Status` AS Status,
     tteacher.`Category` AS Category
FROM
     `tteacher` tteacher
Where
tteacher.`Status`="quit" and
month(tteacher.`QuitDate`)=month(Current_date()) and year(tteacher.`QuitDate`)=year(Current_date())
]]>
	</queryString>
