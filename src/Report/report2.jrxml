	<queryString>
		<![CDATA[SELECT
     tsiswa.`Id`,
     tsiswa.`Name`,
     tclass.`ClassName`,
     tclass.`Time`,
     tteacher.`Name` AS tteacher_Name,
     tclass.`AorB` AS tclass_AorB,
     tclass.`Id` AS tclass_Id
FROM
     `tteacher` tteacher INNER JOIN `tclass` tclass ON tteacher.`Id` = tclass.`TeacherID`
     INNER JOIN `tsiswa` tsiswa ON tclass.`Id` = tsiswa.`Class`
WHERE
     tsiswa.status = 'Active']]>
	</queryString>
