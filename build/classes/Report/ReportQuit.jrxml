	<queryString>
		<![CDATA[select tsiswa.ID, tsiswa.name, tsiswa.class AS 'Class Id', tclass.classname,tclass.time, tteacher.name as 'Teacher', tsiswa.quitdate, tsiswa.reason
from tsiswa
inner join tteacher
inner join tclass on tclass.teacherid = tteacher.id
where tsiswa.status="QUIT" and
month(tsiswa.quitdate)=month('CURRENT_DATE()') and year(tsiswa.quitdate)=year('CURRENT_DATE()')
group by class,DATE_FORMAt( tsiswa.quitDate, "%Y-%m");]]>
	</queryString>
